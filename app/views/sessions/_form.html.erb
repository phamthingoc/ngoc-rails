<%= form_with scope: :session, url: login_path do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, class: "form-control" %>
  <%= f.label :password %>
  <%= f.password_field :password, class: "form-control" %>
  <%= f.submit t "static_pages.sessions.login", class: "btn btn-primary" %>
<% end %>
